<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="britowellington.trickbmx" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme">

        <activity
            android:name=".TelaPrincipal"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".NovaManobra"
            android:label="@string/title_activity_nova_manobra"
            android:parentActivityName=".TelaPrincipal">
        </activity>

        <activity
            android:name=".Detalhes"
            android:label="@string/title_activity_detalhes"
            android:parentActivityName=".TelaPrincipal"
            >
        </activity>

        <activity
            android:name=".Tab2"
            android:label="@string/title_activity_tab2" >
        </activity>

    </application>

</manifest>
